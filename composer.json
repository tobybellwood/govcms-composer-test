	"autoload": {
		"psr-4": {
			"govCMS\\Core\\": "src"
		}
	},
	"config": {
		"vendor-dir": "vendor",
		"sort-packages": false,
		"optimize-autoloader": true
	},
	"conflict": {
		"drupal/drupal": "8.*",
		"drupal/core": "8.*",
		"drush/drush": "9.*"
	},
	"scripts": {
		"post-install-cmd": [
				"PhantomInstaller\\Installer::installPhantomJS",
				"@composer push"
		],
		"post-update-cmd": [
				"PhantomInstaller\\Installer::installPhantomJS",
				"@composer push",
				"@composer package"
		],
		"package": "govCMS\\Core\\Composer\\Package::execute",
		"push": "./vendor/bin/phing push",
		"nuke": "rm -r -f bin docroot vendor *.make"
	}
